doctype html
html
	block head
		include head
	block style
	if workUrl
		link(href=workUrl, rel="stylesheet", type="text/css")
	block vars
	body(class=bodyClass)
		.iconset
			include icons.html

		.page-wrapper
			block header
				include header

			block breadcrumbs

			section.b-main-content
				block content

		block footer
			include footer

		block foot
			include foot
